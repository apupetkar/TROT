@model string
@{
    ViewBag.Title = "Know more";
}

<div class="row">
    <div class="large-12 columns">

        <div class="row">

            <div class="large-8 columns">


                <iframe src="@Model"
                        scrolling="no" 
                        style="width:100%; height:500px; padding-top: 10px; border:0px;"></iframe>

            </div>


            <div class="large-4 columns" style="padding-top: 10px">

                <h3 class="show-for-small">Explore @Controls.logo()</h3>

                <div class="panel">
                    <h4 class="hide-for-small">Discover @Controls.logo()</h4>
                    <hr />
                    <p>
                        Explore and understand @Controls.logo(). 
                    </p>
                    <p>   
                        @Controls.logo() attempts to provide the most updated information 
                        and relevant content on the download portal. 
                   </p>
                    <p>     
                        We would like to
                        hear from you and @Controls.logo() would be glad to provide certain
                        content which interests you relating to @Controls.logo().

                    </p>
                </div>

                <div class="panel">
                    <h4 class="hide-for-small">The @Controls.logo() Book</h4>
                    <hr />
                    <p>
                        Preview the e-book written about @Controls.logo()

                    </p>
                    <a href="#" class="small button">Preview</a>
                </div>


            </div>
    </div>

    <!-- End Header Content -->
    <!-- Search Bar -->

    <div class="row">

        <div class="large-12 columns">
            <div class="radius panel">

                <form>
                    <div class="row collapse">

                        <div class="large-10 small-8 columns">
                            <input type="text" />
                        </div>

                        <div class="large-2 small-3 columns">
                            <a href="#" class="postfix button expand">Search</a>
                        </div>

                    </div>
                </form>

            </div>
        </div>

    </div>

    <!-- End Search Bar -->
    <!-- Thumbnails -->

    <div class="row">

        <div class="large-12 show-for-small columns">
            <h3>Header</h3>
            <hr>
        </div>

        <div class="large-4 small-6 columns">
            <iframe src="https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21110&authkey=ADxabcWxZOiRMWY&em=2" width="100%" height="200px" frameborder="0" scrolling="no"></iframe>
            <div class="panel">
                <p>
                    <a href="@Url.Action("Index", "Download", new { url = "https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21110&authkey=ADxabcWxZOiRMWY&em=2" })">@Controls.logo() Usance</a>

                </p>

                <p>
                    <ul>
                        <li>
                            @Controls.logo() for owners of properties or rent.
                        </li>
                        <li>Convert your property into liquid asset
                        </li>
                        <li>Illustrated working via Spreadsheet
                        </li>
                    </ul>
                </p>
            </div>
        </div>

        <div class="large-4 small-6 columns">
            <iframe src="https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21108&authkey=AN353wo5MlwPMR0&em=2" 
                width="100%" height="200px" frameborder="0" scrolling="no"></iframe>
            <div class="panel">
                <p>
                    <a href="@Url.Action("Index", "Download", new { url = "https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21108&authkey=AN353wo5MlwPMR0&em=2" })">@Controls.logo() HNI Opportunities</a>

                </p>

                <p>
                    <ul>
                        <li>
                            Covers @Controls.logo() trade partnership
                        </li>
                        <li>Summarizes @Controls.logo() Safety 
                        </li>
                        <li>
                            Illustrated working via Spreadsheet
                        </li>
                       
                        
                    </ul>
                </p>
            </div>
        </div>

        <div class="large-4 small-6 columns">
            <iframe src="https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21109&authkey=ALbw8URuZHMEmKI&em=2" width="100%" height="200px" frameborder="0" scrolling="no"></iframe>
            <div class="panel">
                <p>
                    <a href="@Url.Action("Index", "Download", new { url = "https://onedrive.live.com/embed?cid=F28C3659F8F15706&resid=F28C3659F8F15706%21109&authkey=ALbw8URuZHMEmKI&em=2" })">@Controls.logo() Developer</a>

                </p>

                <p>
                    <ul>
                        <li>
                            Real estate developers could sell @Controls.logo() products today without selling their properties
                        </li>
                        <li>Benefits for real estate developers
                        </li>
                        <li>Illustrated working via Spreadsheet
                        </li>
                    </ul>
                </p>
            </div>
        </div>

    </div>

    <!-- End Thumbnails -->
</div>
</div>

